{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint",
    "source.fixAll.stylelint"
  ],
  "files.associations": {
    "*.css": "postcss"
  },
  "css.validate": false,
  "postcss.validate": false,
  "liveServer.settings.root": "/dist"
}
